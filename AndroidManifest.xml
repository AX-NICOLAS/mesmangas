<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="fr.letroll.mesmangas"
	android:versionCode="38"
	android:versionName="1.3.2" android:installLocation="auto">
	<application
